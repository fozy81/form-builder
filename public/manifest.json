{
  "manifest_version": 2,
  "name": "Form Builder App",
  "version": "versionString",
  "short_name": "Form Builder",
  "theme_color": "#030154",
  "background_color": "#ffffff",
  "scope": "/",
  "start_url": "/",
  "default_locale": "en",
  "description": "Form Builder is a simple Hoodie app for making forms you can use offline",
  "display": "standalone",
  "icons": [
   {
     "src": "icon_48.png",
     "sizes": "48x48",
     "type": "image/png"
   },
   {
     "src": "icon_520.png",
     "sizes": "520x520",
     "type": "image/png"
   }
 ],
  "orientation": "portrait-primary",
  "web_accessible_resources" : ["icon_48.png","icon_520.png"]
}
